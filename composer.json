    "require": {
        "symfony/yaml": "*",
        "symfony/console": "*",
        "symfony/finder": "*",
        "doctrine/common": "*", 
        "doctrine/orm": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },    
    "authors": [
        {
            "name": "Vsevolod Dolgopolov",
            "email": "zavalit@gmail.com"
        }
    ],
    "bin": ["bin/yamlfixtures", "bin/yamlfixtures.php"],
